{
  "name": "pay-frontend",
  "description": "Payments Frontend application",
  "version": "0.0.1-SNAPSHOT",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">5.6.0"
  },
  "jshintConfig": {
    "node": true,
    "esversion": 6,
    "globals": {
      "describe": false,
      "it": false,
      "before": false,
      "beforeEach": false,
      "after": false,
      "afterEach": false
    }
  },
  "scripts": {
    "prestart": "grunt default",
    "start": "forever --minUptime 1000 --spinSleepTime 500 start.js",
    "stop": "forever stop start.js",
    "watch": "chokidar app test *.js --initial -c 'npm run test'",
    "test": "grunt lint && npm run grunt-test",
    "lint": "node ./node_modules/.bin/jshint --verbose ./app/*.js ./test/*.js",
    "grunt-test": "node ./node_modules/.bin/grunt test"
  },
